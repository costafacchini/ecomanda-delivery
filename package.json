{
  "name": "ecomanda-delivery",
  "version": "0.0.0",
  "private": true,
  "author": "Alan Costa Facchini",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node server.js",
    "build": "cd client && yarn && yarn run build",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "bash -c 'docker-compose up -d && trap \"docker-compose stop\" SIGINT && nodemon server.js'",
    "dev:worker": "nodemon worker.js",
    "test": "NODE_ENV=test jest --no-coverage --maxWorkers=50%",
    "test:ci": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --maxWorkers=50%",
    "watch": "NODE_ENV=test jest --no-coverage --verbose --watch --runInBand --notify --notifyMode always",
    "linter": "yarn eslint 'src/app/**/*.{js,ts,tsx}' 'src/config/**/*.{js,ts,tsx}' 'src/setup/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "fetch-mock": "9.11.0",
    "jest": "27.4.4",
    "mongodb-memory-server": "8.0.4",
    "mongodb-memory-server-global": "8.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "supertest": "6.1.6"
  },
  "dependencies": {
    "archiver": "5.3.0",
    "aws-sdk": "2.1045.0",
    "bcrypt": "5.0.1",
    "bull-board": "2.1.3",
    "bullmq": "1.57.0",
    "cors": "2.8.5",
    "debug": "4.3.3",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-validator": "6.13.0",
    "fishery": "2.1.0",
    "http-errors": "1.8.1",
    "isomorphic-fetch": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "moment": "2.29.1",
    "mongodb": "4.2.1",
    "mongoose": "6.1.1",
    "morgan": "1.10.0",
    "newrelic": "8.6.0",
    "node-notifier": "10.0.0",
    "nodemon": "2.0.15",
    "pg": "8.7.1",
    "redis": "4.0.0",
    "rollbar": "2.24.0",
    "uuid": "8.3.2",
    "yarn": "1.22.17"
  },
  "_moduleAliases": {
    "@models": "./src/app/models",
    "@controllers": "./src/app/controllers",
    "@routes": "./src/app/routes",
    "@config": "./src/config",
    "@queries": "./src/app/queries",
    "@reports": "./src/app/reports",
    "@helpers": "./src/app/helpers"
  }
}
