{
  "name": "ecomanda-delivery",
  "version": "0.0.0",
  "private": true,
  "author": "Alan Costa Facchini",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "node server.js",
    "build": "cd client && yarn && yarn run build",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "bash -c 'docker-compose up -d && trap \"docker-compose stop\" SIGINT && nodemon server.js'",
    "dev:worker": "nodemon worker.js",
    "test": "NODE_ENV=test jest --no-coverage --maxWorkers=50%",
    "test:ci": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --maxWorkers=50%",
    "watch": "NODE_ENV=test jest --no-coverage --verbose --watch --runInBand --notify --notifyMode always",
    "linter": "yarn eslint 'src/app/**/*.{js,ts,tsx}' 'src/config/**/*.{js,ts,tsx}' 'src/setup/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jest-dom": "4.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "fetch-mock": "9.11.0",
    "jest": "27.4.7",
    "jest-date-mock": "1.0.8",
    "mongodb-memory-server": "8.2.0",
    "mongodb-memory-server-global": "8.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "supertest": "6.2.2"
  },
  "dependencies": {
    "@sinonjs/fake-timers": "9.0.0",
    "archiver": "5.3.0",
    "aws-sdk": "2.1066.0",
    "bcrypt": "5.0.1",
    "bull-board": "2.1.3",
    "bullmq": "1.68.0",
    "cors": "2.8.5",
    "debug": "4.3.3",
    "dotenv": "14.3.2",
    "express": "4.17.2",
    "express-validator": "6.14.0",
    "fishery": "2.1.0",
    "http-errors": "2.0.0",
    "isomorphic-fetch": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "moment": "2.29.1",
    "mongodb": "4.3.1",
    "mongoose": "6.1.8",
    "morgan": "1.10.0",
    "newrelic": "8.7.1",
    "node-notifier": "10.0.0",
    "nodemon": "2.0.15",
    "pg": "8.7.1",
    "redis": "4.0.2",
    "rollbar": "2.24.0",
    "socket.io": "4.4.1",
    "uuid": "8.3.2",
    "yarn": "1.22.17"
  },
  "_moduleAliases": {
    "@models": "./src/app/models",
    "@controllers": "./src/app/controllers",
    "@routes": "./src/app/routes",
    "@config": "./src/config",
    "@queries": "./src/app/queries",
    "@reports": "./src/app/reports",
    "@helpers": "./src/app/helpers",
    "@plugins": "./src/app/plugins"
  }
}
