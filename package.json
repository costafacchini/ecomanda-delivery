{
  "name": "ecomanda-delivery",
  "version": "0.0.0",
  "private": true,
  "author": "Alan Costa Facchini",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "node server.js",
    "build": "cd client && yarn && yarn run build",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "bash -c 'docker compose up -d && trap \"docker compose stop\" SIGINT && nodemon server.js'",
    "dev:worker": "nodemon worker.js",
    "test": "NODE_ENV=test jest --no-coverage --maxWorkers=50%",
    "test:ci": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --maxWorkers=50%",
    "watch": "NODE_ENV=test jest --no-coverage --verbose --watch --runInBand --notify --notifyMode always",
    "linter": "yarn eslint 'src/app/**/*.{js,ts,tsx}' 'src/config/**/*.{js,ts,tsx}' 'src/setup/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-promise": "7.2.1",
    "fetch-mock": "10.1.1",
    "jest": "30.0.5",
    "jest-date-mock": "1.0.10",
    "mongodb-memory-server": "10.2.0",
    "mongodb-memory-server-global": "10.2.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "supertest": "7.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.32.0",
    "@aws-sdk/lib-storage": "^3.32.0",
    "@aws-sdk/s3-request-presigner": "^3.32.0",
    "@bull-board/api": "^6.12.0",
    "@bull-board/express": "^6.12.0",
    "@sinonjs/fake-timers": "14.0.0",
    "amqplib": "0.10.9",
    "archiver": "7.0.1",
    "bcrypt": "6.0.0",
    "bl": "^6.0.7",
    "bullmq": "5.58.2",
    "cors": "2.8.5",
    "debug": "4.4.1",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "express-validator": "7.2.1",
    "fishery": "2.3.1",
    "http-errors": "2.0.0",
    "isomorphic-fetch": "3.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "module-alias": "2.2.3",
    "moment": "2.30.1",
    "moment-timezone": "0.6.0",
    "mongodb": "6.19.0",
    "mongoose": "8.18.0",
    "morgan": "1.10.1",
    "newrelic": "13.1.0",
    "node-notifier": "10.0.1",
    "nodemon": "3.1.10",
    "pg": "8.16.3",
    "redis": "5.8.2",
    "rollbar": "2.26.4",
    "socket.io": "4.8.1",
    "uuid": "11.1.0",
    "yarn": "1.22.22"
  },
  "_moduleAliases": {
    "@models": "./src/app/models",
    "@controllers": "./src/app/controllers",
    "@routes": "./src/app/routes",
    "@config": "./src/config",
    "@queries": "./src/app/queries",
    "@reports": "./src/app/reports",
    "@helpers": "./src/app/helpers",
    "@plugins": "./src/app/plugins",
    "@repositories": "./src/app/repositories"
  }
}
