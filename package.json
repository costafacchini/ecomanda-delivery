{
  "name": "ecomanda-delivery",
  "version": "0.0.0",
  "private": true,
  "author": "Alan Costa Facchini",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node server.js",
    "build": "cd client && yarn && yarn run build",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "bash -c 'docker compose up -d && trap \"docker compose stop\" SIGINT && nodemon server.js'",
    "dev:worker": "nodemon worker.js",
    "test": "NODE_ENV=test jest --no-coverage --maxWorkers=50%",
    "test:ci": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --maxWorkers=50%",
    "watch": "NODE_ENV=test jest --no-coverage --verbose --watch --runInBand --notify --notifyMode always",
    "linter": "yarn eslint 'src/app/**/*.{js,ts,tsx}' 'src/config/**/*.{js,ts,tsx}' 'src/setup/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-n": "17.9.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.2.0",
    "fetch-mock": "9.11.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "mongodb-memory-server": "9.3.0",
    "mongodb-memory-server-global": "9.3.0",
    "npm-run-all2": "6.2.0",
    "prettier": "3.3.2",
    "supertest": "7.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.32.0",
    "@aws-sdk/lib-storage": "^3.32.0",
    "@aws-sdk/s3-request-presigner": "^3.32.0",
    "@sinonjs/fake-timers": "11.2.2",
    "amqplib": "0.10.4",
    "archiver": "7.0.1",
    "bcrypt": "5.1.1",
    "bl": "^6.0.7",
    "bull-board": "2.1.3",
    "bullmq": "5.8.1",
    "cors": "2.8.5",
    "debug": "4.3.5",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-validator": "7.1.0",
    "fishery": "2.2.2",
    "http-errors": "2.0.0",
    "isomorphic-fetch": "3.0.0",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "module-alias": "2.2.3",
    "moment": "2.29.4",
    "moment-timezone": "0.5.45",
    "mongodb": "6.7.0",
    "mongoose": "8.4.1",
    "morgan": "1.10.0",
    "newrelic": "11.19.0",
    "node-notifier": "10.0.1",
    "nodemon": "3.1.3",
    "pg": "8.12.0",
    "redis": "4.6.14",
    "rollbar": "2.26.4",
    "socket.io": "4.7.5",
    "uuid": "10.0.0",
    "yarn": "1.22.22"
  },
  "_moduleAliases": {
    "@models": "./src/app/models",
    "@controllers": "./src/app/controllers",
    "@routes": "./src/app/routes",
    "@config": "./src/config",
    "@queries": "./src/app/queries",
    "@reports": "./src/app/reports",
    "@helpers": "./src/app/helpers",
    "@plugins": "./src/app/plugins",
    "@repositories": "./src/app/repositories"
  }
}
